{
  "use": [
    "postcss-import",
    "postcss-export-vars",
    "autoprefixer",
    "postcss-custom-media",
    "postcss-media-minmax",
    "postcss-custom-properties",
    "postcss-apply",
    "doiuse",
    "immutable-css",
    "postcss-reporter"
  ],
  "input": "./lib/index.css",
  "output": "./test/bundle.css",
  "local-plugins": true,
  "autoprefixer": {
    "browsers": "last 1 versions"
  },
  "postcss-custom-properties": {
    "preserve": true,
    "appendVarliables": true
  },
  "postcss-export-vars": {
    "file": "css",
    "type": "json",
    "matches": []
  },
  "doiuse": {
    "browsers": "last 1 versions"
  },
  "postcss-reporter": {
    "clearMessages": true
  }
}
