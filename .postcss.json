{
  "use": [
    "postcss-import",
    "postcss-export-vars",
    "postcss-cssnext",
    "postcss-apply",
    "cssnano",
    "postcss-reporter"
  ],
  "input": "./lib/index.css",
  "output": "./dist/obsidian.min.css",
  "local-plugins": true,
  "postcss-cssnext": {
    "warnForDuplicates": false,
    "features": {
      "rem": false,
      "customProperties": {
        "preserve": true,
        "appendVarliables": true
      },
      "colorFunction": false,
      "customSelectors": false
    }
  },
  "postcss-export-vars": {
    "file": "css",
    "type": "json",
    "matches": []
  },
  "cssnano": {
    "filterPlugins": false,
    "autoprefixer": false,
    "mergeRules": false,
    "discardEmpty": true,
    "discardComments": {
      "removeAllButFirst": true
    }
  },
  "postcss-reporter": {
    "clearMessages": true
  }
}
